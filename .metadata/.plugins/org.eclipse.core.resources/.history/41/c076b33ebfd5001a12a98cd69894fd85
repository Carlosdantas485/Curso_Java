package aplication;

import java.util.Locale;
import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int medida_1_T1, medida_2_T1, medida_3_T1, x, y, z;
		
		System.out.println("Digite as medidas do primeiro trinagulo");
		medida_1_T1 = sc.nextDouble();
		medida_2_T1 = sc.nextDouble();
		medida_3_T1 = sc.nextDouble();
		
		System.out.println("Digite as medidas do segundo trinagulo");
		x = sc.nextDouble();
		y = sc.nextDouble();
		z = sc.nextDouble();
		
		
		
		sc.close();
	}

}
