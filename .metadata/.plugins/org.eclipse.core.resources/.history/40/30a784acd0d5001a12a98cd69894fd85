package entidades;

public class triangulo {
	
	public double medida_1;
	public double medida_2;
	public double medida_3;
	
	public static double area() {
		double aux = (medida_1 + b + c) / 2.0;
		double result = Math.sqrt(aux * (aux - medida_1) * (aux - medida_2) * (aux - medida_3));
		return result;
	}
}
